{
  "private": "true",
  "name": "nuxt_to_next",
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "dev": "wsrun -a -p package-* -r dev",
    "dev:nuxt": "yarn workspace @nuxt run dev",
    "dev:next": "yarn workspace @next run dev"
  },
  "devDependencies": {
    "wsrun": "^5.2.4"
  }
}
